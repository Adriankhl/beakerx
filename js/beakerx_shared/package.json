{
  "name": "beakerx_shared",
  "version": "1.0.0",
  "description": "BeakerX - shared files",
  "author": "Two Sigma Open Source, LLC",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "yarn run clean:lib",
    "clean:lib": "rimraf lib",
    "build": "yarn run clean && yarn run build:ts",
    "build:ts": "tsc -b",
    "prepublish": "yarn run build"
  },
  "dependencies": {
    "@phosphor/commands": "^1.6.1",
    "@phosphor/widgets": "^1.6.0",
    "moment-timezone": "^0.5.13"
  },
  "devDependencies": {
    "@types/moment-timezone": "^0.5.12",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.d.ts",
    "lib/*.d.ts.map",
    "lib/*.js",
    "lib/*.js.map"
  ],
  "directories": {
    "lib": "lib/"
  }
}
