{
  "name": "beakerx",
  "version": "1.4.1",
  "description": "BeakerX: Beaker Extensions for Jupyter",
  "author": "Two Sigma Open Source, LLC",
  "main": "src/index.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "scripts": {
    "clean": "rimraf node_modules/beakerx_shared && yarn install --force",
    "build": "yarn run build:dev",
    "build:prod": "yarn run clean && webpack --config webpack.prod.js && yarn run build-lab-extension",
    "build:dev": "yarn run clean && webpack --config webpack.dev.js && yarn run build-lab-extension",
    "build-lab-extension": "cd ../lab && npm install",
    "prepublishOnly": "yarn run build:prod"
  },
  "devDependencies": {
    "@jupyter-widgets/base": "^1.2.4",
    "@jupyter-widgets/controls": "^1.2.1",
    "@types/d3": "^5.0.0",
    "@types/jquery": "^3.3.0",
    "@types/jqueryui": "^1.12.2",
    "@types/node": "^12.0.4",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.2.0",
    "html-loader": "^0.5.5",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.11.0",
    "require-hacker": "^3.0.1",
    "sass-loader": "^7.1.0",
    "sinon": "^4.2.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^4.5.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.4.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@jupyterlab/coreutils": "^1.0.10",
    "@phosphor/datagrid": "^0.1.5",
    "@phosphor/datastore": "^0.7.0",
    "@phosphor/widgets": "^1.5.0",
    "beakerx_shared": "./../beakerx_shared",
    "big.js": "^3.1.3",
    "d3": "^5.9.2",
    "flatpickr": "^4.5.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.13",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.3",
    "underscore": "^1.8.3"
  }
}
